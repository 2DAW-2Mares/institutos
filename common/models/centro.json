{
  "name": "Centro",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "codigo": {
      "type": "number",
      "required": true
    },
    "nombre": {
      "type": "string",
      "required": true
    },
    "web": {
      "type": "string"
    },
    "situacion": {
      "type": "geopoint"
    }
  },
  "validations": [],
  "relations": {
    "anyoescolars": {
      "type": "hasMany",
      "model": "Anyoescolar",
      "foreignKey": "centro"
    },
    "usuario": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
